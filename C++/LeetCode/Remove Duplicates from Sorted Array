// below solution is optimized 
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        //vector<int> ans;
        if (nums.size() <1) {
            return 0;
        }
       int j = 1;
        for (int i = 0; i < nums.size(); ++i) {
            if ((i+1) < nums.size() && nums[i] != nums[i + 1]  ) {
               
                    nums[j] = nums[i+1];
                    ++j;
               
            }
        }
        return j;   
    }
};
